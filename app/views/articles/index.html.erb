<h1>WIKIWAGON</h1>

<% @articles.each do |article| %>
  <h2><%= article.title %></h2>
  <p><%= article.content %></p>
  <p><%= link_to "article", article_path(article) %>
    <%= link_to "edit", edit_article_path(article) %>
    <%= link_to "delete", article_path(article), method: :delete%></p>
<% end %>
